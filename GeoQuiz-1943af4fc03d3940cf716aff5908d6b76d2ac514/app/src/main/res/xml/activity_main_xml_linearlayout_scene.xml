<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="300dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:id="@+id/textView3" />
        <Constraint
            motion:layout_editor_absoluteY="340dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="110dp"
            android:layout_width="wrap_content"
            android:id="@+id/button" />
        <Constraint
            motion:layout_editor_absoluteY="287dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="130dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView4" />
        <Constraint
            motion:layout_editor_absoluteY="248dp"
            motion:layout_editor_absoluteX="130dp"
            android:id="@+id/linearLayout2" />
        <Constraint
            motion:layout_editor_absoluteY="356dp"
            motion:layout_editor_absoluteX="1dp"
            android:id="@+id/linearLayout4" />
        <Constraint
            motion:layout_editor_absoluteY="356dp"
            motion:layout_constraintEnd_toEndOf="@+id/button2"
            android:id="@+id/linearLayout5"
            motion:layout_constraintStart_toStartOf="@+id/button" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>